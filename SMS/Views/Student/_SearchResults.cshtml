@if (Model != null && Model.Count > 0)
{
    <div class="table-responsive">
        <table class="table table-bordered table-hover">
            <thead>
                <tr>
                    <th>Registration No</th>
                    <th>First Name</th>
                    <th>Middle Name</th>
                    <th>Last Name</th>
                    <th>Display Name</th>
                    <th>Email</th>
                    <th>Gender</th>
                    <th>DOB</th>
                    <th>Address</th>
                    <th>Contact No</th>
                    <th>Status</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var student in Model)
                {
                    <tr>
                        <td>@student.StudentRegNo</td>
                        <td>@student.FirstName</td>
                        <td>@student.MiddleName</td>
                        <td>@student.LastName</td>
                        <td>@student.DisplayName</td>
                        <td>@student.Email</td>
                        <td>@student.Gender</td>
                        <td>@student.DOB</td>
                        <td>@student.Address</td>
                        <td>@student.ContactNo</td>
                        <td>
                            @if (student.IsEnable)
                            {
                                <button type="button" class="btn btn-success" onclick="toggleEnable('@student.StudentID', false)">Enabled</button>

                            }
                            else
                            {
                                <button type="button" class="btn btn-danger" onclick="toggleEnable('@student.StudentID', true)">Disabled</button>
                            }
                        </td>
                        <td>
                            <button type="button" class="btn btn-primary" onclick="editSubject('@student.StudentID')"><i class="bi bi-pencil-fill"></i></button>
                            <button type="button" class="btn btn-danger" onclick="deleteSubject('@student.StudentID')"><i class="bi bi-trash"></i></button>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
else
{
    <p>No results found.</p>
}



